#include <iostream>
#include <string>
#include <sstream>
using namespace std;

bool massive(int array[10])
{
    string string;
    getline( cin, string );
    istringstream stream( string );
    bool result = true;
    for( int i = 0; i < 10; ++i ) {
        if ( !(stream >> array[ i ]) ) {
            result = false;
            break;
        }
    }
    return result;
}
int decrease(int array[10]) {
    for(int i = 0; i < 10; i++) {
        if (!(array[i] >= array[i-1])) {cout << "The input sequence must be non-decreasing."; exit(0); }
    }
    return 0;
}

