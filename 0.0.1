#include <iostream>
#include <string>
using namespace std;

int main()
{
    string string;
    getline(cin, string);
    if ( string.length() > 80) {cout << "Error"; exit(0);}
    for(int i = 0; i<string.length(); i++)
        if (string[i] == ' ') string.replace(string.find(" "),1,"%20");
    cout << string;
}
